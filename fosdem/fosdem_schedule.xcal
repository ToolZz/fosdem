      <pentabarf:title>Xen on ARM</pentabarf:title>
      <pentabarf:subtitle>Virtualization for the Automotive industry</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Automotive development</pentabarf:track>
      <pentabarf:start>2014-02-02 11:45:00 +0100</pentabarf:start>
      <pentabarf:end>2014-02-02 12:25:00 +0100</pentabarf:end>
