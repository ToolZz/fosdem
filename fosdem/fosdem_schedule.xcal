      <status>CONFIRMED</status>
      <categories>Energy-efficient computing</categories>
      <url>https:/fosdem.org/2014/schedule/event/speedo/</url>
      <location>AW1.126</location>
      <attendee>David Greaves</attendee>
    </vevent>
