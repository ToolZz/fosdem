      <pentabarf:title>Linux tracing with LTTng</pentabarf:title>
      <pentabarf:subtitle>The love of development without printf()</pentabarf:subtitle>
      <pentabarf:language>en</pentabarf:language>
      <pentabarf:language-code>en_US</pentabarf:language-code>
      <pentabarf:track>Tracing and debugging</pentabarf:track>
      <pentabarf:start>2014-02-01 13:00:00 +0100</pentabarf:start>
